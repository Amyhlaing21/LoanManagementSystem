@model IEnumerable<LoanManagementSystem.Models.Borrower><h2>Borrowers</h2><p>    @Html.ActionLink("Create New Borrower", "Create", null, new { @class = "btn btn-success" })</p><table class="table table-bordered table-striped">    <thead>        <tr>            <th>Full Name</th>            <th>Email</th>            <th>Phone</th>            <th>Address</th>            <th>Identification Number</th>            <th>Actions</th>        </tr>    </thead>    <tbody>        @foreach (var b in Model)        {<tr>    <td>@b.FullName</td>    <td>@b.Email</td>    <td>@b.Phone</td>    <td>@b.Address</td>    <td>@b.IdentificationNumber</td>    <td>        @Html.ActionLink("Edit", "Edit", new { id = b.Id }, new { @class = "btn btn-primary btn-sm" }) |        @Html.ActionLink("Delete", "Delete", new { id = b.Id }, new { @class = "btn btn-danger btn-sm" })    </td></tr>}    </tbody></table>