@model LoanManagementSystem.Models.InterestRate@{    ViewBag.Title = "Delete Interest Rate";}<h2>Delete Interest Rate</h2><h3>Are you sure you want to delete this?</h3><div>    <h4>Rate: @Model.RatePercent %</h4>    <h4>Description: @Model.Description</h4></div>@using (Html.BeginForm("DeleteConfirmed", "InterestRate", FormMethod.Post)){@Html.AntiForgeryToken()@Html.HiddenFor(model => model.Id)<input type="submit" value="Delete" class="btn btn-danger" />@Html.ActionLink("Cancel", "Index", null, new { @class = "btn btn-default" })}