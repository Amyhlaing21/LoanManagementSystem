@model IEnumerable<LoanManagementSystem.Models.InterestRate>@{    ViewBag.Title = "Interest Rates";}<h2>Interest Rates</h2><p>    @*@Html.ActionLink("Create New", "Create")*@    <a href="@Url.Action("Create")" class="btn btn-success">Create</a></p><table class="table table-striped">    <tr>        <th>ID</th>        <th>Rate (%)</th>        <th>Description</th>        <th>Actions</th>    </tr>    @foreach (var item in Model)    {<tr>    <td>@item.Id</td>    <td>@item.RatePercent</td>    <td>@item.Description</td>    <td>        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |        @Html.ActionLink("Delete", "Delete", new { id = item.Id })    </td></tr>}</table>